<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fillViewport="true">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.sym.login.GetTreatmentFromNoti"
        android:background="#A93226">
        <TextView
            android:layout_width="fill_parent"
            android:layout_height="80dp"
            android:layout_marginTop="20dp"
            android:text="UPDATE PHYSICAL EXAMINATION"
            android:fontFamily="serif-monospace"
            android:textColorHighlight="#ffffff"
            android:textSize="28dp"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:textStyle="bold"
            />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:padding="10dp"
        >
        <TableRow>
            <TextView
                android:id="@+id/getRegNo"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="53dp"
                android:text="Register Number"
                android:textSize="18dp"
                android:textStyle="bold"
                />

            <EditText
                android:id="@+id/editGetRegNo"
                android:layout_width="270dp"
                android:layout_height="50dp"
                android:layout_alignRight="@id/getRegNo"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="53dp"
                android:background="@drawable/btn_capsule_shape"
                android:paddingLeft="5dp"
                android:textColor="#ffffff"
                android:textSize="18dp" />
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/condition"
                android:layout_width="100dp"
                android:layout_height="70dp"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="53dp"
                android:text="General Condition"
                android:textSize="18dp"
                android:textStyle="bold"
                />

            <EditText
                android:id="@+id/editgetcondition"
                android:layout_width="270dp"
                android:layout_height="50dp"
                android:layout_alignRight="@id/condition"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="53dp"
                android:text=""
                android:textSize="18dp"
                android:background="@drawable/btn_capsule_shape"
                android:textColor="#ffffff"
                android:paddingLeft="5dp"/>
        </TableRow>
        <TableRow>
            <TextView
                android:id="@+id/temperature"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="2dp"
                android:text="Fever"
                android:textSize="18dp"
                android:textStyle="bold"
                />
            <EditText
                android:id="@+id/getTemperature"
                android:layout_width="260dp"
                android:layout_height="50dp"
                android:layout_alignRight="@id/temperature"
                android:layout_marginTop="10dp"
                android:text=""
                android:textSize="18dp"
                android:layout_marginLeft="5dp"
                android:background="@drawable/btn_capsule_shape"
                android:textColor="#ffffff"
                android:paddingLeft="5dp"

                />
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/anaemia"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginLeft="2dp"
                android:text="Anaemia"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                />

            <EditText
                android:id="@+id/getanaemia"
                android:layout_width="260dp"
                android:layout_height="50dp"
                android:layout_alignRight="@id/anaemia"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/btn_capsule_shape"
                android:textColor="#ffffff"
                android:paddingLeft="5dp"
