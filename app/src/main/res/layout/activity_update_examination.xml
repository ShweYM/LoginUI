                android:background="@drawable/btn_capsule_shape"
                android:textColor="#ffffff"
                android:paddingLeft="5dp"
                android:text="fluza - 1dp , para - 2 dp, aaa - 3 dp , dddd - 5 dp , eee - 8 dp , fffffffffffffffffff"
