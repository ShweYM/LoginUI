<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sym.login">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".successlogin" />
        <activity android:name=".successregister" />
        <activity android:name=".doctorlogin" />
        <activity android:name=".sellerlogin" />
        <activity android:name=".RegisterUI" />
        <activity android:name=".GeneralPatientRegister" />
        <activity android:name=".Prediction" />
        <activity android:name=".CNPatient" />
        <activity android:name=".GetTreatmentFromNoti" />
        <activity android:name=".Notitest" />
        <activity android:name=".PulmonaryPatient" />
        <activity android:name=".GastroPatient" />
        <activity android:name=".HepatoPatient" />
        <activity android:name=".CardioPatient" />
        <activity android:name=".UpdateExamination"></activity>
    </application>

</manifest>